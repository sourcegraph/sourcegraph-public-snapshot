{
  "compilerOptions": {
    "noEmit": false,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "out",
    "lib": ["es6"],
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"],
}

{
  "name": "sourcegraph-platform",
  "displayName": "Sourcegraph",
  "description": "Sourcegraph for VS Code",
  "version": "1.0.10",
  "publisher": "sourcegraph",
  "license": "MIT",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/sourcegraph-vscode.git"
  },
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Sourcegraph extension configuration",
      "properties": {
        "lsp.trace": {
          "type": "boolean",
          "default": false,
          "description": "Print trace logs of all LSP communication with the language server"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.113",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "execa": "^0.6.3",
    "isomorphic-fetch": "^2.2.1",
    "opn": "^5.0.0",
    "universal-websocket-client": "^1.0.2",
    "uuid": "^3.2.1",
    "vscode": "^1.1.18",
    "vscode-jsonrpc": "^3.6.2",
    "vscode-languageclient": "^4.2.1",
    "vscode-languageserver-types": "^3.8.2"
  }
}

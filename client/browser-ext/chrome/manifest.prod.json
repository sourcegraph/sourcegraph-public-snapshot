{
  "version": "1.1.76",
  "name": "Sourcegraph for GitHub",
  "manifest_version": 2,
  "description": "Gives GitHub IDE powers: jump to definition and hover tooltips in code, PRs, and diffs",
  "browser_action": {
    "default_title": "Sourcegraph"
  },
  "applications": {
    "gecko": {
      "id": "firefox-extension@sourcegraph.com"
    }
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://sourcegraph.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "js/inject.bundle.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.bundle.js"
    ]
  },
  "permissions": [
    "cookies",
    "storage",
    "https://github.com/*",
    "https://sourcegraph.com/*"
  ],
  "content_security_policy": "default-src 'self'; script-src 'self'; connect-src 'self' https://sourcegraph-logging.telligentdata.com;"
}
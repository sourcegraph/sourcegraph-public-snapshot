  "scripts": {
    "start": "NODE_ENV=${NODE_ENV:=development} node ./node_modules/.bin/webpack-dev-server",
    "check": "flow check",
    "build": "env NODE_ENV=production node ./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint --max-warnings=0 web_modules",
    "test": "npm run check && npm run lint && npm run test-unit",
    "test-unit": "find web_modules -name '*_test.js' | env NODE_PATH=web_modules NODE_ENV=test xargs ./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --require ignore-styles --require isomorphic-fetch",
    "dep": "npm rebuild fsevents && node ./node_modules/flow-bin/lib/install.js",
    "graph": "mkdir -p artifacts/; ./node_modules/.bin/babel --no-babelrc --presets react --out-dir artifacts/compiled/ web_modules/; ./node_modules/.bin/madge artifacts/compiled/ -p artifacts/compiled/ -f es6 -i artifacts/app-graph.png -x 'node_modules|_test|util|Dispatcher|Actions|Component'; rm -r artifacts/compiled/",
    "overwrite-autotest": "for f in `find ./web_modules -name '*.json.actual'`; do mv \"$f\" \"${f%.actual}\"; done"
  },
  "browser": {
    "fs": false,
    "child_process": false
  }

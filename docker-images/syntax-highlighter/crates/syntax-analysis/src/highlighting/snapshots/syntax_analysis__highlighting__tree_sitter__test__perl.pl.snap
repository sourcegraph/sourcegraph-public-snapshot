// ^^ Keyword
//    ^^ Identifier
//       ^ IdentifierOperator
//         ^^^^^^^^^^^^ IdentifierType
//                       ^^^ IdentifierFunction
   $c->add(m_domain => ".example.com", m_scheme => "http", verbose => 1);
// ^^ Identifier
//     ^^^ IdentifierFunction
//         ^^^^^^^^^ StringLiteral
//                  ^^ IdentifierOperator
//                     ^^^^^^^^^^^^^^ StringLiteral
//                                     ^^^^^^^^^ StringLiteral
//                                              ^^ IdentifierOperator
//                                                 ^^^^^^ StringLiteral
//                                                         ^^^^^^^^ StringLiteral
//                                                                 ^^ IdentifierOperator
//                                                                    ^ NumericLiteral
  
   use HTTP::Request;
// ^^^ Keyword
//     ^^^^^^^^^^^^^ IdentifierType
   my $request = HTTP::Request->new(GET => "http://www.example.com");
// ^^ Keyword
//    ^^^^^^^^ Identifier
//             ^ IdentifierOperator
//               ^^^^^^^^^^^^^ IdentifierType
//                              ^^^ IdentifierFunction
//                                  ^^^^ StringLiteral
//                                      ^^ IdentifierOperator
